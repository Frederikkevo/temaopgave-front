---
import BaseLayout from "../layouts/BaseLayout.astro";
import EmployeeSection from "../components/EmployeeSection.astro";
import "../styles/global.css";
const coreData = await fetch(
  "https://raw.githubusercontent.com/Frederikkevo/frontend-design/main/resources/json/corevalues.json"
).then((res) => res.json());
const projectionsData = await fetch(
  "https://raw.githubusercontent.com/Frederikkevo/frontend-design/main/resources/json/projections.json"
).then((res) => res.json());
const employeeData = await fetch(
  "https://raw.githubusercontent.com/Frederikkevo/frontend-design/main/resources/json/employees.json"
).then((res) => res.json());
---

<BaseLayout>
  <EmployeeSection data={employeeData} theme="dark" />
</BaseLayout>
